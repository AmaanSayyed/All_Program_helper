Number = 012333
  
  Amount = 20
  
  Output =  
  01 * 20 = 20
  23 * 20 = 460
  33 * 20 = 660
  
   Code
   
   #include <bits/stdc++.h>
using namespace std;

int main() {
	int n; string s;
    cin >> s >> n;
    for(int i=0; i<s.length(); i+=2) {
        int ans = (s[i]-'0') * 10 + (s[i+1]-'0');
        cout << ans*n << endl;
    }
	return 0;
}
  
